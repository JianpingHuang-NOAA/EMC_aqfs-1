# @(#)$Header$
AR = ar
CC = cc  -64
FC = f77 -64
M4 = m4

PVM_ROOT = /env/proj/ppar/pvm3
PVM_ARCH = SGI64
M4DEFFILE = $(PVM_ROOT)/conf/$(PVM_ARCH).m4
OMPFLAGS  = -mp -mpio
OMPLIBS   = -mp -mpio -lmp
COPTFLAGS = -g
FOPTFLAGS = -g -C -OPT:Olimit=0
FPPFLAGS  = -cpp -macro_expand
FSFLAGS   = -static
ARCHFLAGS = -align64 -r12000 -TARG:platform=ip27,processor=r12000
PARFLAGS  =
ARCHLIB   = 
PVMLIBS   = -L$(PVM_ROOT)/lib/$(PVM_ARCH) -lpvm3
